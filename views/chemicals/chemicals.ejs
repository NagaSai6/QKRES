    <script src="https://cdnjs.cloudflare.com/ajax/libs/noty/3.1.4/noty.min.js"></script>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Merriweather:ital,wght@1,700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@700&family=Montserrat:wght@300&display=swap" rel="stylesheet">
</head>
<body style="margin-top: 60px;">
    <%-include("../navbar.ejs");  %>
   
    <section id="header">

    <div class="ctube">
        <img class="chemicaltube" src="/images/chemistry.svg" alt="qkres chemicals animated svg">
    </div>
    <img class="hatom" src="/images/atom.svg" alt="qkres chemicals atom svg">
    <img class="hflask" src="/images/flask.svg" alt="qkres chemicals flask svg">
  <!-- <img class="hchemical" src="/images/chemical.svg" alt="chemicalImg"> -->
    <div class="chead">
        <h1 id="cheading" >Chemicals</h1>
    </div>

    </section>
    <section id="csection2">
     <h1>We at <span id="qkresCN">QkRes</span> , serve you with great Care</h1>
    
     <div id="cs2d1">
        <h4>Things you can expect</h4>
        <p>Industrial chemicals and products such as Remi,Whatman, Aquasol </p>
     </div>
     <div id="cs2d2">
        <h4>Chemical lab Equipments</h4>
        <p>Chromatography Products, Laboratory Apparel, Laboratory Machines,..... </p>
        <a class="btn btn-sm cb" href="/chemical-lab-equipments">See More</a>
     </div>
    </section>
    <section id="csection3">
    <div class="cs3d1"> 
        <div class="cs3d2">
            <p id="cs3p">Chemicals</p>
          </div>
          <div class="ccontainer">
 <% data.forEach(function(c){ %>
    <div class="ccard">
        <div class="cbox">
            <div class="ccontent">
            <img style="height: 64px;" src="/images/molecule.svg" alt="qkres chemical image">
            <h3> <%= c.name %> </h3>
            <p>Enter required quantity in litres or kg's</p>
           <form>
              <input id="<%=c.name%>" class="form__input" type="text"  required placeholder="eg: 23">
           </form>
            <button class="add_To_Cart btn btn-lg cbutton" data-x="<%= JSON.stringify(c) %>" >Add to cart</button>
            </div>
        </div>
    </div>  

 <% })  %>   
          </div>
    </div>

    </section>
    <%- include("../footer.ejs") %>
    <script src="/js/main.js"></script>
</body>
</html>